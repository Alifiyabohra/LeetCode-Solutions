int findContentChildren(vector<int>& g, vector<int>& s) {
        int m=s.size();
        int n=g.size();
        sort(s.begin(),s.end());
        sort(g.begin(),g.end());
        int l=0,r=0;
        while(l<m && r<n){
            if(s[l]>=g[r]){
                r++;
            }
            l++;
        }
        return r;
    }
