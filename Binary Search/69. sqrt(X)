link--> https://leetcode.com/problems/sqrtx/description/

class Solution {
public:
    int mySqrt(int x){
        int s=0;
        int e=x;
         long long  root=-1;
       
        while(s<=e){
            long long  mid=s+(e-s)/2;
            long long product = mid*mid; 
            if(mid*mid == x){
                root=mid;
                return root;
            }
            else if(mid*mid < x){
                root=mid;
                s=mid+1;
            }
            else{
                e=mid-1;
            }
        }
        return root;
    }
