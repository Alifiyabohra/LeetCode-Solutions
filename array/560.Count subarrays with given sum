int subarraySum(vector<int>& arr, int k){
        int n=arr.size();
        int sum=0;
        int count=0;
        map<int,int>mpp;
        mpp[0]=1;
        for(int i=0;i<n;i++){
            sum+=arr[i];

            int rem=sum-k;
            if(mpp.find(rem)!=mpp.end()){
                count+=mpp[rem];
            }
             mpp[sum]++;
            
        }
        return count;
    }
