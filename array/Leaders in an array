#include <bits/stdc++.h> 
vector<int> findLeaders(vector<int> &elements, int n) {
    // Write your code here.
    int maxi=elements[n-1];
   
    vector<int>ans;
    ans.push_back(elements[n-1]);
    for(int i=n-2;i>=0;i--){
        if(elements[i]>maxi){
            maxi=elements[i];
            ans.push_back(elements[i]);
        }
    }
    reverse(ans.begin(),ans.end());
    return ans;
}
